{
    "name": "infobip-community/infobip-api-php-sdk",
    "description": "PHP SDK package for Infobip API",
    "keywords": ["php", "sdk", "client", "package", "infobip", "api"],
    "homepage": "https://github.com/infobip-community/infobip-api-php-sdk",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Frano Sasvari",
            "email": "sasvari.frano@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19",
        "friendsofphp/php-cs-fixer": "3.4.*",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3",
        "illuminate/support": "7.*",
        "overtrue/phplint": "^3.0",
        "phpoption/phpoption": "^1.8",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-mockery": "^1.0",
        "vlucas/phpdotenv": "^4.2",
        "ergebnis/phpstan-rules": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Infobip\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infobip\\Support\\Laravel\\InfobipServiceProvider"
            ]
        }
    }
}
